{
  "name": "@manifold-studio/react-manifold",
  "version": "0.1.0",
  "description": "A custom React renderer for declarative CSG modeling using manifold-3d",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "three": ">=0.150.0"
  },
  "peerDependenciesMeta": {
    "three": {
      "optional": true
    }
  },
  "dependencies": {
    "manifold-3d": "^3.0.0",
    "react-reconciler": "^0.29.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-reconciler": "^0.33.0",
    "@types/three": "^0.160.0",
    "react": "^18.2.0",
    "three": "^0.160.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "react",
    "csg",
    "manifold",
    "3d",
    "modeling",
    "threejs",
    "react-three-fiber"
  ],
  "license": "MIT"
}
