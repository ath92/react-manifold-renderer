#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/manifold-3d@3.3.2/node_modules/manifold-3d/bin/node_modules:/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/manifold-3d@3.3.2/node_modules/manifold-3d/node_modules:/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/manifold-3d@3.3.2/node_modules:/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/manifold-3d@3.3.2/node_modules/manifold-3d/bin/node_modules:/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/manifold-3d@3.3.2/node_modules/manifold-3d/node_modules:/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/manifold-3d@3.3.2/node_modules:/Users/tomhutman/Documents/2026/react-manifold-renderer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../manifold-3d/bin/manifold-cad" "$@"
else
  exec node  "$basedir/../manifold-3d/bin/manifold-cad" "$@"
fi
